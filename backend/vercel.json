{
  "version": 2,
  "builds": [
    {
      "src": "backend/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "backend/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",

      "src": "/",

      "dest": "/"
    },
    
  ]
}
